{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "BaseSheet",
  "type": "object",
  "properties": {
    "cellHeight": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0
    },
    "cellWidth": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0
    },
    "colCount": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0
    },
    "rowCount": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0
    },
    "sheetId": {
      "type": "string"
    },
    "sheetName": {
      "type": "string"
    },
    "sheetSize": {
      "type": "array",
      "items": {
        "type": "integer",
        "format": "uint32",
        "minimum": 0
      },
      "maxItems": 2,
      "minItems": 2
    },
    "sheetType": {
      "type": "string"
    }
  },
  "required": [
    "sheetId",
    "sheetSize",
    "sheetType",
    "sheetName",
    "rowCount",
    "colCount",
    "cellWidth",
    "cellHeight"
  ]
}